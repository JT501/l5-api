{
  "name": "specialtactics/l5-api",
  "description": "Dependencies for the Laravel 5 API Boilerplate package",
  "keywords": ["laravel", "boilerplate", "api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Max Snow",
      "email": "contact@maxsnow.me"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "dingo/api": "^2",
    "tymon/jwt-auth": "^1.0",
    "illuminate/support": "^5.8",
    "webpatser/laravel-uuid": "^3.0",
    "ramsey/uuid": "^3.0",
    "atehnix/laravel-stubs": "~6.0"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.5",
    "orchestra/testbench": "^3.8",
    "fideloper/proxy": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Specialtactics\\L5Api\\": "src/"
    },
    "files": ["helpers/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Specialtactics\\L5Api\\Tests\\": "tests/",
      "App\\": "tests/app/"
    },
    "classmap": ["tests/database/seeds/", "tests/database/factories/"]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Specialtactics\\L5Api\\L5ApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "./vendor/bin/phpunit --colors=always -v --testdox"
  }
}
